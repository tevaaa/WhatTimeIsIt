{
    "name": "What time is it ?",
    "description": "An extension that show you the time all around the world !",
    "version": "1.0",
    "manifest_version": 3,
    "background" : 
    {
        "service_worker" : "js/background.js"
    },
    
    "action":
    {
        "default_popup": "web/popup.html",
        "default_icon": 
        {
            "16": "https://ibb.co/JBbTYsw"
        }
    },
    "permissions": [
        "storage"
      ]
}